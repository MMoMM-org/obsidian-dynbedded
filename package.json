{
	"author": "Marcus Breiden <marcus@mmomm.org",
	"repository": "https://github.com/MMoMM-Marcus/obsidian-dynbedded.git",
	"dependencies": {
	},
	"description": "Dynamic Embeds for Obsidian.md",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"esbuild-plugin-copy": "1.3.0",
		"obsidian": "latest",
		"process": "^0.11.10",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"semantic-release": "19.0.5",
		"@semantic-release/commit-analyzer": "9.0.2",
		"@semantic-release/release-notes-generator": "10.0.3",
		"@semantic-release/changelog": "6.0.1",
		"conventional-changelog-eslint": "3.0.9"
	},
	"keywords": [],
	"license": "GPLv3",
	"main": "src/main.js",
	"name": "obsidian-dynbedded",
	"scripts": {
		"dev": "node esbuild.dev.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"version": "1.0.0",
	"release": {
		"dryRun": true,
		"private": true,
		"preset": "eslint",
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			["@semantic-release/npm", {
				"npmPublish": false
			}],
			["@semantic-release/github",
				{
					"assets": [
						{
							"path": "build/**",
							"name": "obsidian-plugin-base",
							"label": "${nextRelease.gitTag}"
						}
					]
				}
			]
		]
	}
}
